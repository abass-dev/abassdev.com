{
    "rewrites": [
        {
            "source": "/blog/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "abassdev.com"
                }
            ],
            "destination": "http://server6.stormerhost.com/~abassdev/blog/:path*"
        },
        {
            "source": "/blog/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "www.abassdev.com"
                }
            ],
            "destination": "http://server6.stormerhost.com/~abassdev/blog/:path*"
        }
    ],
    "routes": [
        {
            "src": "/ads.txt",
            "headers": {
                "Content-Type": "text/plain"
            },
            "dest": "/ads.txt"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}